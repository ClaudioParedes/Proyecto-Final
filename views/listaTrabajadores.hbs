<div class="row container px-sm-5 py-5 justify-content-center mx-auto">
    <div class="my-1"><h1>Lista de Trabajadores</h1></div>
    <hr />
<form class="border shadow col-6 col-md-10 col-lg-6">
      <div class="container">
          <div class="row align-items-center my-5">
         {{#each trabajadores}}     
<div class="col-12 col-6">
    <ul class="list-group list-group-horizontal">
     <li class="list-group-item list-group-item-dark">{{this.id_trabajador}}</li>
     <li class="list-group-item list-group-item-dark ">{{this.rut}}</li>
     <li class="list-group-item list-group-item-dark">{{this.nombre}} {{this.apellido}}</li>
    </ul>
</div>
         {{/each}}
             
          </div>
     </div>  
<div class="container">
          <div class="row align-items-right my-5">
              <div class="col-12 col-md-6">
                <button id="volver" type="submit" class="btn btn-primary">Volver</button>
               </div>
         </div>
</div>
</form>
</div>
</div>

<script>
    const volver = document.getElementById("volver")
    volver.addEventListener("click", async (e) => {
        e.preventDefault()
        window.location = `/menuAdmin`
    });
</script>
